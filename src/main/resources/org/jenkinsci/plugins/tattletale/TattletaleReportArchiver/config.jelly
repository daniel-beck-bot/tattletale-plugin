<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">
 
  <f:entry title="Directory for archiving data (relative to build dir)" field="storeDir">
    <f:textbox default="tattletale-report" />
  </f:entry>
  
  <f:entry title="Retain Tattletale report for each successful build">
	<f:checkbox name="keepAll" checked="${descriptor.isKeepAll()}" />
  </f:entry>
  
</j:jelly>
